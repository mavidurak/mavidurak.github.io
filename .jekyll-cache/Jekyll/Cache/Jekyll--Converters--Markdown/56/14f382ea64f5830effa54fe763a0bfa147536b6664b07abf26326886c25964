<span class="kd">function</span> <span class="nx">KontrolEt</span><span class="p">(</span><span class="nx">callback</span><span class="p">:</span><span class="nx">IFiltre</span><span class="p">)</span> <span class="p">{</span>
	<span class="kd">var</span> <span class="nx">result</span> <span class="o">=</span> <span class="nx">callback</span><span class="p">(</span><span class="mi">2</span><span class="p">);</span>
