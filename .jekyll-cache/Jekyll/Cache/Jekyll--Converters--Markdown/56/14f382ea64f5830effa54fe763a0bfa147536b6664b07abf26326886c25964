	<span class="k">if</span><span class="p">(</span><span class="nx">x</span><span class="o">&gt;</span><span class="mi">5</span><span class="p">)</span> <span class="k">return</span> <span class="kc">true</span><span class="p">;</span>
	<span class="k">return</span> <span class="kc">false</span><span class="p">;</span>
